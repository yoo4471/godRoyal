            success: function (response) {
                if (response == "done") {
                    window.top.location.href = 'http://localhost:3000/reserve/complete/<%=email%>';
                } else {
                    // alert(response + "Form submission failed!");
                }
            },
            error: function (response) {
                alert(response);
            }
        });

      }


    });

